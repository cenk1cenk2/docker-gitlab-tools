[
  [
    {
      "name": "config",
      "commands": ["/etc/cont-init.d/00-generate-configuration.sh"]
    },
    {
      "name": "migrations",
      "commands": ["/etc/cont-init.d/10-migrate-database.sh"]
    },
    {
      "name": "permissions",
      "commands": ["/etc/cont-init.d/20-fix-attrs.sh"]
    }
  ],
  [
    {
      "name": "nginx",
      "commands": ["service nginx start"]
    }
  ],
  [
    {
      "name": "celerybeat",
      "commands": ["/etc/services.d/gitlab-tools-bgtask/service.sh"],
