        "gid": 900
      }
    },
    {
      "name": "celeryworker",
      "commands": ["/etc/services.d/gitlab-tools-worker/service.sh"],
      "cwd": "/opt/gitlab-tools",
      "run_as": {
        "enable": true,
        "uid": 900,
        "gid": 900
      }
    },
    {
      "name": "uwsgi",
      "commands": ["/etc/services.d/uwsgi/service.sh"],
      "cwd": "/opt/gitlab-tools",
      "run_as": {
        "enable": true,
        "uid": 900,
        "gid": 900
      }
    }
  ]
]
